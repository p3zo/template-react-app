{
    "name": "template-react-app",
    "version": "0.1.0",
    "private": true,
    "author": "p3zo",
    "description": "A quick jumping-off point for React apps",
    "main": "index.js",
    "scripts": {
        "build": "webpack",
        "start": "webpack serve --config ./webpack.config.js --mode development",
        "serve": "serve --single --listen 8000 dist",
        "lint": "node node_modules/eslint/bin/eslint.js src --ext '.js,.jsx'"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/preset-env": "^7.25.3",
        "@babel/preset-react": "^7.24.7",
        "babel-loader": "^9.1.3",
        "css-loader": "^7.1.2",
        "react-hot-loader": "^4.13.1",
        "style-loader": "^4.0.0",
        "webpack": "^5.93.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "babel": {
        "ignore": [
            "node_modules"
        ],
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
